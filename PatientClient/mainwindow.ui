  <property name="styleSheet">
   <string notr="true">/* ---- 主窗口样式 ---- */
QMainWindow {
    background-color: #f5f7fa; /* 设置一个柔和的浅灰色背景 */
}

/* ---- 欢迎语标签 ---- */
QLabel#welcomeLabel {
    font-size: 16px;
    color: #555;
    padding-left: 10px;
}
QLabel#patientNameLabel {
    font-size: 16px;
    font-weight: bold;
    color: #2c3e50;
}

/* ---- 核心功能按钮 (预约挂号) ---- */
QPushButton#mainAppointmentButton {
    background-color: #3498db; /* 主题蓝色 */
    color: white;
    font-size: 20px;
    font-weight: bold;
    border: none;
    border-radius: 12px; /* 圆角 */
    padding: 10px;
    margin: 15px;
}
QPushButton#mainAppointmentButton:hover {
    background-color: #2980b9; /* 鼠标悬浮时颜色变深 */
}
QPushButton#mainAppointmentButton:pressed {
    background-color: #1f618d; /* 鼠标按下时颜色再变深 */
}

/* ---- 次要功能网格按钮 (QToolButton) ---- */
QToolButton {
    background-color: white;
    color: #333;
    border: 1px solid #e0e0e0;
    border-radius: 10px;
    padding: 10px;
    font-size: 13px;
}
QToolButton:hover {
    background-color: #ecf5ff; /* 鼠标悬浮时淡蓝色背景 */
    border: 1px solid #3498db;
}
QToolButton:pressed {
    background-color: #d9ecff; /* 鼠标按下时 */
}</string>
